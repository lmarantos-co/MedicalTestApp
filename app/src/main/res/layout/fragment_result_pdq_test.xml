    <!--    covariance title form-->

    <RelativeLayout
        android:id="@+id/pdqTestTitleRelLayout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:gravity="center"
        app:layout_constraintLeft_toRightOf="@id/titleVGL1"
        app:layout_constraintRight_toLeftOf="@id/titleVGL2"
        app:layout_constraintTop_toBottomOf="@id/resultsTopHGL">


        <TextView
            android:id="@+id/totalPSScore"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/pdq_relative_layout"
            android:gravity="center"
            android:paddingLeft="5dp"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:text="@string/pdq_test_results"
            android:textColor="@color/black"
            android:textSize="20sp" />


        <LinearLayout
            android:id="@+id/PDQScoreLinLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/totalPSScore"
            android:background="@color/light_gray"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingLeft="10dp"
            android:paddingTop="10dp"
            android:paddingRight="10dp"
            android:paddingBottom="10dp">


            <TextView
                android:id="@+id/totalScoreDescTxtV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="false"
                android:gravity="center"
                android:text="@string/pdq_test_score"
                android:textColor="@color/black"
                android:textSize="20sp" />

        </LinearLayout>

    </RelativeLayout>

