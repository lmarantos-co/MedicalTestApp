<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000"
        motion:motionInterpolator="easeIn">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/medicalStartHGL"
            android:orientation="horizontal"
            motion:layout_constraintGuide_percent="0.2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:transitionEasing="cubic(0.2,0.2,1,1)" />

        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/Medical"
            motion:layout_constraintBottom_toTopOf="@id/medicalStartHGL"
            motion:layout_constraintRight_toLeftOf="@id/medIcal"
            android:layout_marginEnd="108dp" />

        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/mEdical"
            motion:layout_constraintBottom_toTopOf="@id/medicalStartHGL"
            motion:layout_constraintRight_toLeftOf="@id/medIcal"
            android:layout_marginEnd="72dp" />

        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/meDical"
            motion:layout_constraintBottom_toTopOf="@id/medicalStartHGL"
            motion:layout_constraintRight_toLeftOf="@id/medIcal"
            android:layout_marginEnd="36dp" />

        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/medIcal"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/medicalStartHGL" />

        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/mediCal"
            android:layout_marginStart="36dp"
            motion:layout_constraintBottom_toTopOf="@id/medicalStartHGL"
            motion:layout_constraintLeft_toRightOf="@id/medIcal" />

        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/medicAl"
            android:layout_marginStart="72dp"
            motion:layout_constraintBottom_toTopOf="@id/medicalStartHGL"
            motion:layout_constraintLeft_toRightOf="@id/medIcal" />

        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/medicaL"
            android:layout_marginStart="108dp"
            motion:layout_constraintBottom_toTopOf="@id/medicalStartHGL"
            motion:layout_constraintLeft_toRightOf="@id/medIcal" />

        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/Test"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintRight_toLeftOf="@+id/testStartVGL"
            android:layout_marginEnd="72dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tEst"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintRight_toLeftOf="@+id/testStartVGL"
            android:layout_marginEnd="36dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/teSt"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintLeft_toRightOf="@+id/testStartVGL"
            android:layout_marginStart="36dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tesT"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintLeft_toRightOf="@+id/testStartVGL"
            android:layout_marginStart="72dp" />

        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/Estimator"
            motion:layout_constraintTop_toBottomOf="@+id/estimatorStartHGL"
            motion:layout_constraintRight_toLeftOf="@id/estiMator"
            android:layout_marginEnd="144dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/eStimator"
            motion:layout_constraintTop_toBottomOf="@+id/estimatorStartHGL"
            motion:layout_constraintRight_toLeftOf="@id/estiMator"
            android:layout_marginEnd="108dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/esTimator"
            motion:layout_constraintTop_toBottomOf="@+id/estimatorStartHGL"
            motion:layout_constraintRight_toLeftOf="@id/estiMator"
            android:layout_marginEnd="72dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/estImator"
            motion:layout_constraintTop_toBottomOf="@+id/estimatorStartHGL"
            motion:layout_constraintRight_toLeftOf="@id/estiMator"
            android:layout_marginEnd="36dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/estiMator"
            motion:layout_constraintTop_toBottomOf="@id/estimatorStartHGL"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"/>
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/estimAtor"
            motion:layout_constraintTop_toBottomOf="@+id/estimatorStartHGL"
            motion:layout_constraintLeft_toRightOf="@id/estiMator"
            android:layout_marginStart="36dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/estimaTor"
            motion:layout_constraintTop_toBottomOf="@+id/estimatorStartHGL"
            motion:layout_constraintLeft_toRightOf="@id/estiMator"
            android:layout_marginStart="72dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/estimatOr"
            motion:layout_constraintTop_toBottomOf="@+id/estimatorStartHGL"
            motion:layout_constraintLeft_toRightOf="@id/estiMator"
            android:layout_marginStart="108dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/estimatoR"
            motion:layout_constraintTop_toBottomOf="@+id/estimatorStartHGL"
            motion:layout_constraintLeft_toRightOf="@id/estiMator"
            android:layout_marginStart="144dp" />

        <Constraint
            android:id="@+id/signatureTxtV"
            android:alpha="0"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/signatureHGL">
        <CustomAttribute
            motion:attributeName="alpha"
            motion:customFloatValue="0.0" />
        </Constraint>
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">

        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/Medical"
            motion:layout_constraintBottom_toTopOf="@id/medicalEndHGL"
            motion:layout_constraintRight_toLeftOf="@id/medIcal"
            android:layout_marginEnd="48dp" />

        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/mEdical"
            motion:layout_constraintBottom_toTopOf="@id/medicalEndHGL"
            motion:layout_constraintRight_toLeftOf="@id/medIcal"
            android:layout_marginEnd="32dp" />

        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/meDical"
            motion:layout_constraintBottom_toTopOf="@id/medicalEndHGL"
            motion:layout_constraintRight_toLeftOf="@id/medIcal"
            android:layout_marginEnd="16dp" />

        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/medIcal"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/medicalEndHGL" />

        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/mediCal"
            android:layout_marginStart="16dp"
            motion:layout_constraintBottom_toTopOf="@id/medicalEndHGL"
            motion:layout_constraintLeft_toRightOf="@id/medIcal" />

        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/medicAl"
            android:layout_marginStart="32dp"
            motion:layout_constraintBottom_toTopOf="@id/medicalEndHGL"
            motion:layout_constraintLeft_toRightOf="@id/medIcal" />

        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/medicaL"
            android:layout_marginStart="48dp"
            motion:layout_constraintBottom_toTopOf="@id/medicalEndHGL"
            motion:layout_constraintLeft_toRightOf="@id/medIcal" />

        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/Test"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintRight_toLeftOf="@+id/testStartVGL"
            android:layout_marginEnd="32dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tEst"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintRight_toLeftOf="@+id/testStartVGL"
            android:layout_marginEnd="16dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/teSt"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintLeft_toRightOf="@+id/testStartVGL"
            android:layout_marginStart="16dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tesT"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintLeft_toRightOf="@+id/testStartVGL"
            android:layout_marginStart="32dp" />

        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/Estimator"
            motion:layout_constraintTop_toBottomOf="@+id/estimatorEndHGL"
            motion:layout_constraintRight_toLeftOf="@id/estiMator"
            android:layout_marginEnd="64dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/eStimator"
            motion:layout_constraintTop_toBottomOf="@+id/estimatorEndHGL"
            motion:layout_constraintRight_toLeftOf="@id/estiMator"
            android:layout_marginEnd="48dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/esTimator"
            motion:layout_constraintTop_toBottomOf="@+id/estimatorEndHGL"
            motion:layout_constraintRight_toLeftOf="@id/estiMator"
            android:layout_marginEnd="32dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/estImator"
            motion:layout_constraintTop_toBottomOf="@+id/estimatorEndHGL"
            motion:layout_constraintRight_toLeftOf="@id/estiMator"
            android:layout_marginEnd="16dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/estiMator"
            motion:layout_constraintTop_toBottomOf="@id/estimatorEndHGL"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"/>
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/estimAtor"
            motion:layout_constraintTop_toBottomOf="@+id/estimatorEndHGL"
            motion:layout_constraintLeft_toRightOf="@id/estiMator"
            android:layout_marginStart="16dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/estimaTor"
            motion:layout_constraintTop_toBottomOf="@+id/estimatorEndHGL"
            motion:layout_constraintLeft_toRightOf="@id/estiMator"
            android:layout_marginStart="32dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/estimatOr"
            motion:layout_constraintTop_toBottomOf="@+id/estimatorEndHGL"
            motion:layout_constraintLeft_toRightOf="@id/estiMator"
            android:layout_marginStart="48dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/estimatoR"
            motion:layout_constraintTop_toBottomOf="@+id/estimatorEndHGL"
            motion:layout_constraintLeft_toRightOf="@id/estiMator"
            android:layout_marginStart="64dp" />

        <Constraint
            android:id="@+id/signatureTxtV"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/signatureHGL">
            <CustomAttribute
                motion:attributeName="alpha"
                motion:customFloatValue="1.0" />
        </Constraint>

    </ConstraintSet>
</MotionScene>